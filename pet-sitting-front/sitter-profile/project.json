{
  "name": "sitter-profile",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "sitter-profile/src",
  "prefix": "pet-sitting-front",
  "tags": [],
  "projectType": "library",
  "targets": {
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "sitter-profile/jest.config.ts"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "sitter-profile/**/*.ts",
          "sitter-profile/**/*.html"
        ]
      }
    }
  }
}
